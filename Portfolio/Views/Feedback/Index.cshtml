@model IEnumerable<Portfolio.Models.Feedback>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center">Feedbacks about my works</h2> 
<a href="Feedback/New" class="btn_contact" style="width:50%"><span>Rate my work!</span></a>
<div class="container-fluid">
    <div class="row">
        @foreach (var f in Model)
        {
            @Html.Partial("_Feedback", f)
        }

    </div>
</div>
 
 